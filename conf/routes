# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index


GET        /flux                controllers.Application.miseAJourFlux

GET        /createsites         controllers.Application.insertSites


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)


GET        /create                          controllers.Application.create
GET        /get/france                      controllers.Application.getNodeOfFrance
GET        /get                             controllers.Application.getAllNodes
GET        /delete                          controllers.Application.delete
GET        /createUser                      controllers.Application.createUser
GET        /deleteUser                      controllers.Application.deleteUser
GET        /createEntite                    controllers.Application.createEntite
GET        /createAppreciationEntite        controllers.Application.createAppreciationEntite
GET        /testBDD                         controllers.Application.estFavoriAppreciationEntite
GET        /annotation/test     controllers.Application.test
GET        /presentation        controllers.Application.presentation